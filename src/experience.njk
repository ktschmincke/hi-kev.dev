<article>
  <h1 class="screen-only">Experience</h1>

  <div class="[ contact ] [ print-only ]">
    <h1 class="name">{{name}}</h1>
    <div class="contact__info">
      <a href="mailto:{{email}}">{{email}}</a>
      <br>
      <a href="https://{{metadata.url | url}}">{{metadata.url}}</a>
    </div>
  </div>

  <hr class="print-only">

  <h2>Summary</h2>
  <p>{{summary}}</p>

  <h2>Skills</h2>
  <ul class="skills">
    {% for skill in skills %}
      <li>{{skill}}</li>
    {% endfor %}
  </ul>

  <h2>Experience</h2>
  <ul class="[ unstyled-list ] [ flow ]">
    {% for job in experience %}
      <li class="[ experience ] [ flow ]">
        <div class="experience__header">
          <strong>
            <span class="experience__primary">{{job.company}}</span>
            &mdash;
            <em>{{job.title}}</em>
          </strong>

          <p class="experience__time">{{job.time}}</p>
        </div>

        <ul class="experience__items">
          {% for item in job.items %}
            <li>{{item}}</li>
          {% endfor %}
        </ul>
      </li>
    {% endfor %}
  </ul>

  <h2>Education</h2>
  <ul class="[ unstyled-list ] [ flow ]">
    {% for item in education %}
      <li class="experience">
        <div class="experience__header">
          <strong>
            <span class="experience__primary">{{item.school}}</span>
            &mdash;
            <ul class="[ experience__ed-degrees ] [ unstyled-list d-inline ]">
              {% for degree in item.degrees %}
                <li class="d-inline"><em>{{degree}}</em></li>
              {% endfor %}
            </ul>
          </strong>
          <p class="experience__time">{{item.time}}</p>
        </div>
      </li>
    {% endfor %}
  </ul>

  <h2>Certifications</h2>
  <ul class="[ unstyled-list ] [ flow ]">
    {% for item in certifications %}
