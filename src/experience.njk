        <div class="experience__header">
          <strong>
            <span class="experience__primary">{{item.name}}</span>
            &mdash;
            <em>{{item.organization}}</em>
          </strong>
          <p class="experience__time">{{item.date}}</p>
        </div>
      </li>
    {% endfor %}
  </ul>

</article>
